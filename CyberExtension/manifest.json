{
  "manifest_version": 3,
  "name": "CyberShield Extension",
  "version": "1.0",
  "permissions": [
    "tabs"
  ],
  "host_permissions": [
    "https://www.virustotal.com/*/"
  ],
  "icons": {
    "16": "icons/Cyber16.png",
    "32": "icons/Cyber32.png",
    "48": "icons/Cyber48.png",
    "128": "icons/Cyber128.png" 
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
